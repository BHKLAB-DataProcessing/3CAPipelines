{
    "pipeline": {
      "name": "upload3CA"
    },
    "transform": {
      "image": "bhklab/python_zenodo",
      "cmd": [
            "python3",
            "/pfs/ZenodoUpload/upload_multi.py",
            "get3CA",
            "3CA Data",
            "Curated Cancer Cell Atlas datasets from Weizmann Institute of Science generated by ORCESTRA."
        ]
    },
    "resource_requests": {
      "memory": "100M"
    },
    "input": {
      "cross": [
        {
          "git": {
            "url": "https://github.com/BHKLAB-Pachyderm/ZenodoUpload.git",
            "name": "",
            "branch": "master"
          }
        },
        {
            "pfs": {
              "repo": "get3CA",
              "glob": "/"
            }
        }
      ]
    }
}